    <style name="zxing_AlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
    <item name="android:textColor">@android:color/black</item>
    <item name="android:textSize">16sp</item>
    <item name="android:fontFamily">@font/syne_regular</item>
    <item name="android:dialogMessage">@font/syne_regular</item>
    <item name="android:titleTextColor" ns1:targetApi="m">@android:color/black</item>
    <item name="buttonBarNegativeButtonStyle">@style/zxing_NegativeButtonStyle</item>
    <item name="buttonBarPositiveButtonStyle">@style/zxing_PositiveButtonStyle</item>
    <item name="android:layoutDirection">locale</item>
    <item name="android:textDirection">locale</item>
  </style>
    <style name="zxing_CaptureTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
    <!-- Primary brand color. -->
    <item name="colorPrimary">@color/zxing_purple_500</item>
    <item name="colorPrimaryVariant">@color/zxing_purple_700</item>
    <item name="colorOnPrimary">@color/zxing_white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/zxing_teal_200</item>
    <item name="colorSecondaryVariant">@color/zxing_teal_700</item>
    <item name="colorOnSecondary">@color/zxing_black</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
    <!-- Customize your theme here. -->
    <item name="android:forceDarkAllowed" ns1:targetApi="q">false</item>
    <item name="android:textColor">@color/zxing_black</item>
    <item name="textAllCaps">false</item>
  </style>
    <style name="zxing_NegativeButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
    <item name="android:textColor">@color/zxing_primary</item>
  </style>
    <style name="zxing_PositiveButtonStyle" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
    <item name="android:textColor">@color/zxing_primary</item>
  </style>
    <declare-styleable name="zxing_camera_preview">
    <attr format="dimension" name="zxing_framing_rect_width"/>
    <attr format="dimension" name="zxing_framing_rect_height"/>
    <attr format="boolean" name="zxing_use_texture_view"/>
    <attr format="enum" name="zxing_preview_scaling_strategy">
      <enum name="centerCrop" value="1"/>
      <enum name="fitCenter" value="2"/>
      <enum name="fitXY" value="3"/>
    </attr>
  </declare-styleable>
    <declare-styleable name="zxing_finder">
    <attr format="color" name="zxing_possible_result_points"/>
    <attr format="color" name="zxing_result_view"/>
    <attr format="color" name="zxing_viewfinder_laser"/>
    <attr format="color" name="zxing_viewfinder_mask"/>
    <attr format="boolean" name="zxing_viewfinder_laser_visibility"/>
  </declare-styleable>
    <declare-styleable name="zxing_view">
    <attr format="reference" name="zxing_scanner_layout"/>
  </declare-styleable>
</resources>